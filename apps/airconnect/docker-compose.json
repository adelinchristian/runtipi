{
  "services": [
    {
      "name": "airconnect",
      "image": "1activegeek/airconnect",
      "isMain": true,
      "internalPort": "",
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "AIRCAST_VAR",
          "value": "user=root -x /config/config.xml -I -f /config/airconnect.log -l 1000:2000 -d all=info"
        }
      ],
      "volumes": [
        {
          "containerPath": "/config",
          "hostPath": "${APP_DATA_DIR}/data/config",
          "readOnly": false,
          "shared": false,
          "private": false
        },
        {
          "containerPath": "/etc/localtime",
          "hostPath": "/etc/localtime",
          "readOnly": true,
          "shared": false,
          "private": false
        }
      ],
      "addPorts": [],
      "networkMode": "host",
      "hostname": "airconnect"
    }
  ],
  "schemaVersion": 2
}
