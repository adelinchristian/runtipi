{
  "services": [
    {
      "name": "musicassistant",
      "image": "ghcr.io/music-assistant/server:latest",
      "isMain": true,
      "internalPort": "8095",
      "environment": [
        {
          "key": "LOG_LEVEL",
          "value": "info"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "PUID",
          "value": "1000"
        }
      ],
      "volumes": [
        {
          "containerPath": "/data",
          "hostPath": "${APP_DATA_DIR}/data",
          "readOnly": false,
          "shared": false,
          "private": false
        },
        {
          "containerPath": "/etc/localtime",
          "hostPath": "/etc/localtime",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "networkMode": "host"
    }
  ],
  "schemaVersion": 2
}
