{
  "services": [
    {
      "name": "Chromium",
      "image": "lscr.io/linuxserver/chromium:latest",
      "isMain": true,
      "internalPort": "3000",
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "GUID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "containerPath": "/config",
          "hostPath": "${APP_DATA_DIR}/data/config",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "addPorts": [
        {
          "containerPort": "3000",
          "hostPort": "3300",
          "tcp": false,
          "udp": false
        },
        {
          "containerPort": "3001",
          "hostPort": "3301",
          "tcp": false,
          "udp": false
        }
      ],
      "hostname": "chromium"
    }
  ],
  "schemaVersion": 2
}
